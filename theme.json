{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": false,
		"color": {
			"defaultPalette": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"duotone": [],
			"gradients": [],
			"link": false,
			"background": true,
			"text": true,
			"palette": [
				{ "slug": "transparent", "color": "transparent", "name": "None" },
				{ "slug": "white", "color": "#FFFFFF", "name": "White" },
				{ "slug": "black", "color": "#000000", "name": "Black" },
				{ "slug": "cobalt", "color": "#0E18C8", "name": "Cobalt" },
				{ "slug": "indigo", "color": "#2A1BFF", "name": "Indigo" },
				{ "slug": "violet", "color": "#5B00FF", "name": "Violet" },
				{ "slug": "hyper-pink", "color": "#FF006E", "name": "Hyper Pink" },
				{ "slug": "acid", "color": "#B4FF00", "name": "Acid Lime" },
				{ "slug": "signal-red", "color": "#D6002F", "name": "Signal Red" },
				{ "slug": "teal", "color": "#005F5B", "name": "Deep Teal" }
			]
		},
		"spacing": {
			"padding": false,
			"margin": true,
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"slug": "16",
					"size": "var(--spacing-16)",
					"name": "Extra Small (16px)"
				},
				{
					"slug": "32",
					"size": "var(--spacing-32)",
					"name": "Small (32px)"
				},
				{
					"slug": "64",
					"size": "var(--spacing-64)",
					"name": "Medium (64px)"
				},
				{
					"slug": "96",
					"size": "var(--spacing-96)",
					"name": "Large (96px)"
				},
				{
					"slug": "112",
					"size": "var(--spacing-112)",
					"name": "Extra Large (112px)"
				},
				{
					"slug": "224",
					"size": "var(--spacing-224)",
					"name": "Huge (224px)"
				}
			]
		},
		"typography": {
			"fluid": true,
			"customFontSize": false,
			"defaultFontSizes": false,
			"letterSpacing": false,
			"lineHeight": false,
			"dropCap": false,
			"writingMode": false,
			"textTransform": false,
			"textColumns": false,
			"textDecoration": false,
			"fontStyle": false,
			"fontWeight": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Inter\", system-ui, -apple-system, sans-serif",
					"name": "Inter",
					"slug": "inter"
				},
				{
					"fontFamily": "\"Urbanist\", serif",
					"name": "Urbanist",
					"slug": "urbanist"
				}
			],
			"fontSizes": [
				{
					"name": "9XL (80px)",
					"slug": "text-9xl",
					"size": "clamp(2.5rem, 6.25vw, 5rem)"
				},
				{
					"name": "8XL (72px)",
					"slug": "text-8xl",
					"size": "clamp(2.25rem, 5.625vw, 4.5rem)"
				},
				{
					"name": "7XL (64px)",
					"slug": "text-7xl",
					"size": "clamp(2rem, 5vw, 4rem)"
				},
				{
					"name": "6XL (56px)",
					"slug": "text-6xl",
					"size": "clamp(1.75rem, 4.375vw, 3.5rem)"
				},
				{
					"name": "5XL (48px)",
					"slug": "text-5xl",
					"size": "clamp(1.5rem, 3.75vw, 3rem)"
				},
				{
					"name": "4XL (40px)",
					"slug": "text-4xl",
					"size": "clamp(1.25rem, 3.125vw, 2.5rem)"
				},
				{
					"name": "3XL (32px)",
					"slug": "text-3xl",
					"size": "clamp(1.125rem, 2.5vw, 2rem)"
				},
				{
					"name": "2XL (24px)",
					"slug": "text-2xl",
					"size": "clamp(1rem, 1.875vw, 1.5rem)"
				},
				{
					"name": "XL (20px)",
					"slug": "text-xl",
					"size": "clamp(1rem, 1.5625vw, 1.25rem)"
				},
				{
					"name": "LG (18px)",
					"slug": "text-lg",
					"size": "clamp(1rem, 1.40625vw, 1.125rem)"
				},
				{
					"name": "Base (16px)",
					"slug": "text-base",
					"size": "1rem"
				},
				{
					"name": "SM (14px)",
					"slug": "text-sm",
					"size": "0.875rem"
				},
				{
					"name": "XS (12px)",
					"slug": "text-xs",
					"size": "0.75rem"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)"
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--urbanist)"
				}
			}
		}
	}
}
