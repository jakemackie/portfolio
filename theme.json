{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": false,
		"color": {
			"defaultPalette": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"duotone": [],
			"gradients": [],
			"link": false,
			"background": true,
			"text": true,
			"palette": [
				{ "slug": "transparent", "color": "transparent", "name": "None" },
				{ "slug": "black", "color": "#000000", "name": "Black" },
				{ "slug": "white", "color": "#FFFFFF", "name": "White" },
				{ "slug": "grey", "color": "#D3D3D3", "name": "Grey" },
				{ "slug": "primary", "color": "#1e40af", "name": "Primary" },
				{ "slug": "accent", "color": "#06b6d4", "name": "Accent" }
			]
		},
		"spacing": {
			"padding": false,
			"margin": true,
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"slug": "16",
					"size": "var(--spacing-16)",
					"name": "16"
				},
				{
					"slug": "32",
					"size": "var(--spacing-32)",
					"name": "32"
				},
				{
					"slug": "64",
					"size": "var(--spacing-64)",
					"name": "64"
				},
				{
					"slug": "96",
					"size": "var(--spacing-96)",
					"name": "96"
				},
				{
					"slug": "112",
					"size": "var(--spacing-112)",
					"name": "112"
				}
			]
		},
		"typography": {
			"fluid": true,
			"customFontSize": false,
			"defaultFontSizes": false,
			"letterSpacing": false,
			"lineHeight": false,
			"dropCap": false,
			"writingMode": false,
			"textTransform": false,
			"textColumns": false,
			"textDecoration": false,
			"fontStyle": false,
			"fontWeight": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Inter\", system-ui, -apple-system, sans-serif",
					"name": "Inter",
					"slug": "inter"
				},
				{
					"fontFamily": "\"Lora\", serif",
					"name": "Lora",
					"slug": "lora"
				}
			],
			"fontSizes": []
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--black)",
			"text": "var(--wp--preset--color--white)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lora)"
				}
			}
		},
		"blocks": {
			"core/group": {
				"spacing": {
					"margin": {
						"top": "var(--spacing-96)",
						"bottom": "var(--spacing-96)"
					}
				}
			},
			"core/cover": {
				"spacing": {
					"margin": {
						"top": "var(--spacing-96)",
						"bottom": "var(--spacing-96)"
					}
				}
			}
		}
	}
}
