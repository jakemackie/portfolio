{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": false,
		"color": {
			"defaultPalette": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
			"duotone": [],
			"gradients": [],
			"link": false,
			"background": true,
			"text": true,
			"palette": [
				{ "slug": "transparent", "color": "transparent", "name": "None" },
				{ "slug": "black", "color": "#000000", "name": "Black" },
				{ "slug": "white", "color": "#FFFFFF", "name": "White" },
				{ "slug": "grey", "color": "#D3D3D3", "name": "Grey" },
				{ "slug": "primary", "color": "#1e40af", "name": "Primary" },
				{ "slug": "accent", "color": "#06b6d4", "name": "Accent" }
			]
		},
		"spacing": {
			"padding": false,
			"margin": true,
			"customSpacingSize": false,
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"slug": "16",
					"size": "var(--spacing-16)",
					"name": "Extra Small (16px)"
				},
				{
					"slug": "32",
					"size": "var(--spacing-32)",
					"name": "Small (32px)"
				},
				{
					"slug": "64",
					"size": "var(--spacing-64)",
					"name": "Medium (64px)"
				},
				{
					"slug": "96",
					"size": "var(--spacing-96)",
					"name": "Large (96px)"
				},
				{
					"slug": "112",
					"size": "var(--spacing-112)",
					"name": "Extra Large (112px)"
				},
				{
					"slug": "224",
					"size": "var(--spacing-224)",
					"name": "Huge (224px)"
				}
			]
		},
		"typography": {
			"fluid": true,
			"customFontSize": false,
			"defaultFontSizes": false,
			"letterSpacing": false,
			"lineHeight": false,
			"dropCap": false,
			"writingMode": false,
			"textTransform": false,
			"textColumns": false,
			"textDecoration": false,
			"fontStyle": false,
			"fontWeight": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Inter\", system-ui, -apple-system, sans-serif",
					"name": "Inter",
					"slug": "inter"
				},
				{
					"fontFamily": "\"Lora\", serif",
					"name": "Lora",
					"slug": "lora"
				}
			],
			"fontSizes": [
				{
					"name": "9XL (128px)",
					"slug": "text-9xl",
					"size": "clamp(4.5rem, 5.5vw, 8rem)"
				},
				{
					"name": "8XL (96px)",
					"slug": "text-8xl",
					"size": "clamp(3.75rem, 4.5vw, 6rem)"
				},
				{
					"name": "7XL (72px)",
					"slug": "text-7xl",
					"size": "clamp(3rem, 3.75vw, 4.5rem)"
				},
				{
					"name": "6XL (60px)",
					"slug": "text-6xl",
					"size": "clamp(2.625rem, 3.25vw, 3.75rem)"
				},
				{
					"name": "5XL (48px)",
					"slug": "text-5xl",
					"size": "clamp(2.25rem, 2.75vw, 3rem)"
				},
				{
					"name": "4XL (36px)",
					"slug": "text-4xl",
					"size": "clamp(1.875rem, 2.25vw, 2.25rem)"
				},
				{
					"name": "3XL (30px)",
					"slug": "text-3xl",
					"size": "clamp(1.5rem, 1.875vw, 1.875rem)"
				},
				{
					"name": "2XL (24px)",
					"slug": "text-2xl",
					"size": "clamp(1.25rem, 1.5vw, 1.5rem)"
				},
				{
					"name": "XL (20px)",
					"slug": "text-xl",
					"size": "clamp(1.125rem, 1.25vw, 1.25rem)"
				},
				{
					"name": "LG (18px)",
					"slug": "text-lg",
					"size": "clamp(1.0625rem, 1.125vw, 1.125rem)"
				},
				{
					"name": "Base (16px)",
					"slug": "text-base",
					"size": "1rem"
				},
				{
					"name": "SM (14px)",
					"slug": "text-sm",
					"size": "0.875rem"
				},
				{
					"name": "XS (12px)",
					"slug": "text-xs",
					"size": "0.75rem"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)"
		},
		"blocks": {
			"core/heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--lora)"
				}
			}
		}
	}
}
